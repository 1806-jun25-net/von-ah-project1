@model List<Project1.WebApp.Models.OrderWeb>

@{
    ViewData["Title"] = "SearchResultsLocation";
}



<div>
    <h2>Search Results:</h2>
    <hr />
    @foreach (var item in Model)
    {
        <dl class="dl-horizontal">

            <dt>
                @Html.DisplayNameFor(modelItem => item.OrderId)
            </dt>
            <dd>
                @Html.DisplayFor(modelItem => item.OrderId)
            </dd>
            <dt>
                @Html.DisplayNameFor(modelItem => item.Address)
            </dt>
            <dd>
                @Html.DisplayFor(modelItem => item.Address)
            </dd>
            <dt>
                @Html.DisplayNameFor(modelItem => item.UserName)
            </dt>
            <dd>
                @Html.DisplayFor(modelItem => item.UserName)
            </dd>
            <dt>
                @Html.DisplayNameFor(modelItem => item.OrderTime)
            </dt>
            <dd>
                @Html.DisplayFor(modelItem => item.OrderTime)
            </dd>
            <dt>
                @Html.DisplayNameFor(modelItem => item.TotalPrice)
            </dt>
            <dd>
                $@Html.DisplayFor(modelItem => item.TotalPrice)
            </dd>

        </dl>
    }
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
